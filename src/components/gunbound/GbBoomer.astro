<div class="container">
    <h2 class="float">GunBound 2004 - Boomer</h2>
    <form id="dataForm">
        <label class="label-boomer" for="rowInput">Número (1 al 20):</label>
        <input
            type="number"
            id="rowInput"
            min="1"
            max="20"
            placeholder="Ingresa un número del 1 al 20"
        />

        <label class="label-boomer" for="sdSelect">Seleccione SD:</label>
        <select class="Direction" id="sdSelect">
            <option value="1SD">1SD</option>
            <option value="2SD">2SD</option>
        </select>

        <label class="label-boomer" for="categorySelect"
            >Seleccione categoría:</label
        >
        <select class="Direction" id="categorySelect">
            <option value="40">40</option>
            <option value="50">50</option>
            <option value="60">60</option>
            <option value="70">70</option>
            <option value="75">75</option>
            <option value="80">80</option>
            <option value="85">85</option>
            <option value="Fullpower">Full power</option>
            <option value="Timebomb">Time bomb</option>
            <option value="Hook30">Hook 30</option>
            <option value="Hook40">Hook 40</option>
            <option value="Hook50">Hook 50</option>
            <option value="Hook55">Hook 55 1SD</option>
            <option value="Hook60">Hook 60</option>
            <option value="Hooktime">Hook time</option>
            <option value="Hookfull">Hook full</option>
        </select>
    </form>

    <div class="result table-container custom-box">
        Valor: <span id="valueDisplay">N/A</span>
    </div>
</div>
